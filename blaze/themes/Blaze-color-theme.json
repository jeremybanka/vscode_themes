{
  "name": "Blaze",
  "type": "dark",
  "colors": {
    "editor.background": "#000000",
    "tab.activeBackground": "#000000",
    "editorGroupHeader.noTabsBackground": "#000000",
    "input.background": "#000000",
    "sideBar.background": "#1e1c28",
    "titleBar.inactiveBackground": "#1e1c28",
    "titleBar.activeBackground": "#1e1c28",
    "statusBar.background": "#1e1c28",
    "activityBar.background": "#1e1c28",
    "editorGroup.border": "#1e1c28",

    "tab.inactiveBackground": "#3c3a46",
    "editorGroupHeader.tabsBackground": "#3c3a46",

    "activityBar.foreground": "#4b446e",
    "statusBar.foreground": "#b6b4c0",
    "editor.foreground": "#b6b4c0",
    "editorLineNumber.foreground": "#9896a2",
    "editorIndentGuide.background": "#4b446e",
    "editorIndentGuide.activeBackground": "#786eaa",
    "activityBarBadge.background": "#4b446e",
    "progressBar.background": "#00a0f0",
    "editor.selectionBackground": "#00a0f066",
    "editor.selectionHighlightBackground": "#00a0f044",
    "editor.wordHighlightBackground": "#00a0f066",
    "editorGroup.dropBackground": "#00a0f066",
    "sideBarTitle.foreground": "#f5efec",
    "sideBar.foreground": "#f5efec",
    "scrollbar.shadow": "#9896a244",
    "scrollbarSlider.activeBackground": "#9896a299",
    "scrollbarSlider.background": "#9896a266",
    "scrollbarSlider.hoverBackground": "#9896a277",
    "list.activeSelectionBackground": "#464450",
    "list.focusBackground": "#464450",
    "list.inactiveSelectionBackground": "#3c3a46",
    "list.inactiveFocusBackground": "#3c3a46",
    "list.hoverBackground": "#32303c",
    "list.invalidItemForeground": "#ff8269",
    "list.warningForeground": "#ff8269",
    "list.errorForeground": "#ff8269",

    "gitDecoration.addedResourceForeground": "#64c8fa",
    "gitDecoration.modifiedResourceForeground": "#d2aa5a",
    "gitDecoration.deletedResourceForeground": "#960032",
    "gitDecoration.untrackedResourceForeground": "#64c8fa",
    "gitDecoration.ignoredResourceForeground": "#9896a288",
    "gitDecoration.conflictingResourceForeground": "#ff8269",
    "editorError.foreground": "#f03c78",
    "editorWarning.foreground": "#f0641e"
  },
  "tokenColors": [
    {
      "settings": {
        "foreground": "#b6b4c0",
        "background": "#263238"
      }
    },
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "",
        "foreground": "#786eaa"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#f5efec"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#960032"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#f5efec"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword.control",
        "constant.other.color",
        "punctuation",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#9896a2"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#00a0f0"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#d2aa5a"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": ["meta.block variable.other"],
      "settings": {
        "foreground": "#ff8269"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": ["support.other.variable", "string.other.link"],
      "settings": {
        "foreground": "#d23c6e"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "variable.parameter",
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#ff8269"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "fontStyle": "normal",
        "foreground": "#ff8269"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "support.type",
        "support.class",
        "support.orther.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types"
      ],
      "settings": {
        "foreground": "#f03c78"
      }
    },
    {
      "name": "Entity Types",
      "scope": ["support.type"],
      "settings": {
        "foreground": "#ff8269"
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#f0f2f3"
      }
    },
    {
      "name": "Sub-methods",
      "scope": [
        "entity.name.module.js",
        "variable.import.parameter.js",
        "variable.other.class.js"
      ],
      "settings": {
        "foreground": "#4696be"
      }
    },
    {
      "name": "Language methods",
      "scope": ["variable.language"],
      "settings": {
        "fontStyle": "",
        "foreground": "#4696be"
      }
    },
    {
      "name": "entity.name.method.js",
      "scope": ["entity.name.method.js"],
      "settings": {
        "fontStyle": "",
        "foreground": "#4696be"
      }
    },
    {
      "name": "meta.method.js",
      "scope": [
        "meta.class-method.js entity.name.function.js",
        "variable.function.constructor"
      ],
      "settings": {
        "foreground": "#00d2af"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#ff8269"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "text.html.basic entity.other.attribute-name.html",
        "text.html.basic entity.other.attribute-name"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#ffb92d"
      }
    },
    {
      "name": "CSS Classes",
      "scope": ["entity.other.attribute-name.class"],
      "settings": {
        "foreground": "#ff8269"
      }
    },
    {
      "name": "CSS ID's",
      "scope": ["source.sass keyword.control"],
      "settings": {
        "foreground": "#28a08c"
      }
    },
    {
      "name": "Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#28a08c"
      }
    },
    {
      "name": "Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#960032"
      }
    },
    {
      "name": "Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#9678f0"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#9678f0"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#6450c8"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#b6b4c0"
      }
    },
    {
      "name": "ES7 Bind Operator",
      "scope": [
        "source.js constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#503237"
      }
    },
    {
      "name": "JSON Key - Level 0",
      "scope": [
        "source.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
