{
  "name": "Yolk",
  "type": "light",
  "colors": {
    "tab.activeForeground": "#666",
    "editor.background": "#fd0",
    "tab.activeBackground": "#fd0",
    "editorGroupHeader.noTabsBackground": "#000",
    "input.background": "#000",
    "sideBar.background": "#000",
    "titleBar.inactiveBackground": "#000",
    "titleBar.activeBackground": "#000",
    "statusBar.background": "#000",
    "statusBar.noFolderBackground":"#000",
    "activityBar.background": "#000",
    "editorGroup.border": "#000",

    "tab.inactiveBackground": "#000",
    "tab.border": "#000",
    "editorGroupHeader.tabsBackground": "#000",
    "tab.inactiveForeground": "#777",
    "tab.hoverBackground": "#000",

    "activityBar.foreground": "#777",
    "statusBar.foreground": "#777",
    "statusBar.noFolderForeground": "#777",
    "editor.foreground": "#555",
    "editorLineNumber.foreground": "#ccc",
    "editorIndentGuide.background": "#aaa",
    "editorIndentGuide.activeBackground": "#000",
    "activityBarBadge.background": "#000",
    "progressBar.background": "#000",
    "editor.selectionBackground": "#00b4f011",
    "editor.selectionHighlightBackground": "#00b4f011",
    "editor.wordHighlightBackground": "#00b4f011",
    "editorGroup.dropBackground": "#00000011",
    "sideBarTitle.foreground": "#777",
    "sideBar.foreground": "#777",
    "scrollbar.shadow": "#0007",
    "scrollbarSlider.activeBackground": "#aaaaaa99",
    "scrollbarSlider.background": "#aaaaaa66",
    "scrollbarSlider.hoverBackground": "#aaaaaa77",
    "list.activeSelectionBackground": "#fd0",
    "list.activeSelectionForeground": "#777",
    "list.focusBackground": "#000",
    "list.inactiveSelectionBackground": "#000",
    "list.inactiveFocusBackground": "#000",
    "list.hoverBackground": "#000",
    "list.invalidItemForeground": "#f506",
    "list.warningForeground": "#f506",
    "list.errorForeground": "#f056",

    "minimap.background": "#fff",

    "gitDecoration.addedResourceForeground": "#00b4f0",
    "gitDecoration.modifiedResourceForeground": "#d2aa5a",
    "gitDecoration.deletedResourceForeground": "#960032",
    "gitDecoration.untrackedResourceForeground": "#00b4f0",
    "gitDecoration.ignoredResourceForeground": "#9896a288",
    "gitDecoration.conflictingResourceForeground": "#000",
    "editorError.foreground": "#f05",
    "editorWarning.foreground": "#f50"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "",
        "foreground": "#999"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#555"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#555"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#f05"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#555"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword.control",
        "constant.other.color",
        "punctuation",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#555"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#000"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#ff6496"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": ["meta.block variable.other"],
      "settings": {
        "foreground": "#ff8269"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": ["support.other.variable", "string.other.link"],
      "settings": {
        "foreground": "#956"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "variable.parameter",
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#00a0f0"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#555"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "support.type",
        "support.class",
        "support.orther.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types"
      ],
      "settings": {
        "foreground": "#ff6496"
      }
    },
    {
      "name": "Entity Types",
      "scope": ["support.type"],
      "settings": {
        "foreground": "#ff8269"
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#777"
      }
    },
    {
      "name": "Sub-methods",
      "scope": [
        "entity.name.module.js",
        "variable.import.parameter.js",
        "variable.other.class.js"
      ],
      "settings": {
        "foreground": "#00a0f0"
      }
    },
    {
      "name": "Language methods",
      "scope": ["variable.language"],
      "settings": {
        "fontStyle": "",
        "foreground": "#00a0f0"
      }
    },
    {
      "name": "entity.name.method.js",
      "scope": ["entity.name.method.js"],
      "settings": {
        "fontStyle": "",
        "foreground": "#00a0f0"
      }
    },
    {
      "name": "meta.method.js",
      "scope": [
        "meta.class-method.js entity.name.function.js",
        "variable.function.constructor"
      ],
      "settings": {
        "foreground": "#00d2af"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#ff8269"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "text.html.basic entity.other.attribute-name.html",
        "text.html.basic entity.other.attribute-name"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#ffb92d"
      }
    },
    {
      "name": "CSS Classes",
      "scope": ["entity.other.attribute-name.class"],
      "settings": {
        "foreground": "#ff8269"
      }
    },
    {
      "name": "CSS ID's",
      "scope": ["source.sass keyword.control"],
      "settings": {
        "foreground": "#28a08c"
      }
    },
    {
      "name": "Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#28a08c"
      }
    },
    {
      "name": "Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#960032"
      }
    },
    {
      "name": "Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#9678f0"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#9678f0"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#6450c8"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#b6b4c0"
      }
    },
    {
      "name": "ES7 Bind Operator",
      "scope": [
        "source.js constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#5000f0"
      }
    },
